#!/bin/sh

# shellcheck disable=SC1090
[ -f ~/.cache/cpr/colors ] && . ~/.cache/cpr/colors

# shellcheck disable=SC2154
dmenu_run -i -p "launch:" \
    -nb "${color8:=#999999}" \
    -nf "${color15:=#FFFFFF}" \
    -sb "${color8:=#999999}" \
    -sf "${color0:=#000000}" \
    -fn "JetBrainsMono-28"
